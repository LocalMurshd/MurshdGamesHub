local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Murshd Games Hub", HidePremium = false, IntroText = "loud Murshd Hub", SaveConfig = true, ConfigFolder = "Murshd Games Saves"})

OrionLib:MakeNotification({
	Name = "Hey Bro!,Thanks for using Murshd Games Hub",
	Content = "Please Subscribe To Murshd Games for more scripts!!",
	Image = "rbxassetid://17264172243",
	Time = 5
})


-------------window settings end---------------


